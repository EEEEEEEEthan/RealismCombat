; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="RealismCombat"
run/main_scene="res://ProgramRoot.tscn"
config/features=PackedStringArray("4.5", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://Scripts/AudioManager.cs"
DialogueManager="*res://Scripts/Nodes/Dialogues/DialogueManager.cs"

[display]

window/size/viewport_width=256
window/size/viewport_height=224
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="RealismCombat"

[editor_plugins]

enabled=PackedStringArray()

[gui]

theme/custom="res://Theme.tres"

[rendering]

textures/canvas_textures/default_texture_filter=0
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
